<% include head %>
<h2>Users</h2>
<table>
  <tr>
    <th>Email</th>
    <th>Admin</th>
    <th>Points</th>
  </tr>
  <% for (var i = 0; i < users.length; i++) {
    var user = users[i] %>
    <tr>
      <td><%= user.email %></td>
      <td><%= user.admin %></td>
      <td><%= user.points %></td>
    </tr>
  <% } %>
</table>

<% include foot %>
